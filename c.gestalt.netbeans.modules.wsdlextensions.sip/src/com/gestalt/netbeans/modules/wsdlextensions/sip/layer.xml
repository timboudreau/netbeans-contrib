<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">
<filesystem>
    <folder name="WSDLEditor">
        
        <file name="SIPWsdlExt.xsd" url="SIPWsdlExt.xsd">
            <attr name="namespace" stringvalue="http://schemas.sun.com/jbi/wsdl-extensions/sip/" />
            <attr name="prefix" stringvalue="sip" />
        </file>
        
        <folder name ="Binding">
            <file name="SIPBinding">
                <attr name="xsdFileName" stringvalue="SIPWsdlExt" />
                <attr name="element" stringvalue="binding" />
                <attr name="SystemFileSystem.localizingBundle" stringvalue="com.gestalt.netbeans.modules.wsdlextensions.sip.Bundle" />
                <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/com/gestalt/netbeans/modules/wsdlextensions/sip/resources/binding.png" /> 
            </file>
        </folder>
        
        <folder name ="BindingOperation">
            <file name="SIPOperation">
                <attr name="xsdFileName" stringvalue="SIPWsdlExt" />
                <attr name="element" stringvalue="operation" />
                <attr name="SystemFileSystem.localizingBundle" stringvalue="com.gestalt.netbeans.modules.wsdlextensions.sip.Bundle" />
                <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/com/gestalt/netbeans/modules/wsdlextensions/sip/resources/operation.png" /> 
            </file>
        </folder>
        
        <folder name ="BindingOperationInput">
            <file name="SIPInMessage">
                <attr name="xsdFileName" stringvalue="SIPWsdlExt" />
                <attr name="element" stringvalue="input" />
                <attr name="SystemFileSystem.localizingBundle" stringvalue="com.gestalt.netbeans.modules.wsdlextensions.sip.Bundle" />
                <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/com/gestalt/netbeans/modules/wsdlextensions/sip/resources/inMessage.png" /> 
            </file>
        </folder>
        
        <folder name ="ServicePort">
            <file name="SIPPort" >
                <attr name="xsdFileName" stringvalue="SIPWsdlExt" />
                <attr name="element" stringvalue="address" />
                <attr name="SystemFileSystem.localizingBundle" stringvalue="com.gestalt.netbeans.modules.wsdlextensions.sip.Bundle" />
                <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/com/gestalt/netbeans/modules/wsdlextensions/sip/resources/port.png" /> 
            </file>
        </folder>	        
    </folder>
    
    <folder name="SeeBeyondJbiComponents">
        <!-- JBI Component -->
        <folder name="gestalt-sip-binding">
            <attr name="id" stringvalue="gestalt-sip-binding"/>
            <attr name="type" stringvalue="Binding"/>
            <attr name="description" stringvalue="Description of binding component : SIP"/>
            <file name="sip.binding-1.0" >
                <attr name="namespace" stringvalue="http://schemas.sun.com/jbi/wsdl-extensions/sip/"/>
            </file>
        </folder>
    </folder>
    
</filesystem>
