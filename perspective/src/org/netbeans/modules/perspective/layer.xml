<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">
<filesystem>
    <!-- Perspective Actions -->
    <folder name="Actions">
        <folder name="Perspective">
            <!-- Qucik Action -->
            <file name="org-netbeans-modules-perspective-actions-QuickSwitch.instance"/>
            <!-- Reset Action -->
            <file name="org-netbeans-modules-perspective-menus-OpenPerspectivesAction.instance"/>
            <file name="org-netbeans-modules-perspective-menus-RemovePerspectiveAction.instance"/>
            <file name="org-netbeans-modules-perspective-menus-ResetAction.instance"/>
            <!-- Save as Action -->
            <file name="org-netbeans-modules-perspective-menus-ResetPerspectiveAction.instance"/>
            <file name="org-netbeans-modules-perspective-menus-SaveAsAction.instance"/>
        </folder>
    </folder>
    
    <!-- Perspective Menu -->
    <folder name="Menu">
        <folder name="Perspective">
            <!-- i18n attribute -->
            <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.perspective.Bundle"/>
            <!-- position attribute (between tools and window menu)-->
            <attr name="position" intvalue="1150"/>
            <file name="org-netbeans-modules-perspective-menus-OpenPerspectivesAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Perspective/org-netbeans-modules-perspective-menus-OpenPerspectivesAction.instance"/>
                <attr name="position" intvalue="100"/>
            </file>
            <!-- Save as menu -->
            <file name="org-netbeans-perspective-menus-SaveAsAction.shadow">
                <attr name="position" intvalue="500"/>
                <attr name="originalFile" stringvalue="Actions/Perspective/org-netbeans-modules-perspective-menus-SaveAsAction.instance"/>
            </file>
            <file name="org-netbeans-modules-perspective-menus-RemovePerspectiveAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Perspective/org-netbeans-modules-perspective-menus-RemovePerspectiveAction.instance"/>
                <attr name="position" intvalue="600"/>
            </file>
            <file name="org-netbeans-modules-perspective-menus-ResetPerspectiveAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Perspective/org-netbeans-modules-perspective-menus-ResetPerspectiveAction.instance"/>
                <attr name="position" intvalue="1500"/>
            </file>

            <!-- Reset menu -->
            <file name="Separator.instance" >
                <attr name="instanceClass" stringvalue="javax.swing.JSeparator"/>
                <attr name="position" intvalue="1750"/>
            </file>
            <file name="org-netbeans-modules-perspective-menus-ResetAction.shadow">
                <attr name="position" intvalue="2000"/>
                <attr name="originalFile" stringvalue="Actions/Perspective/org-netbeans-modules-perspective-menus-ResetAction.instance"/>
            </file>
        </folder>
    </folder>
    
    <!-- Options Dialog -->
    <folder name="OptionsDialog">
        <!-- Perspective Advanced -->
        <folder name="Advanced">
            <file name="org-netbeans-modules-perspective-options-PerspectiveAdvancedOption.instance"/>
        </folder>
    </folder>
    
    <!-- Shortcuts -->
    <folder name="Shortcuts">
        <!--Quick Switch Shortcut -->
        <file name="OS-S.shadow">
            <attr name="originalFile" stringvalue="Actions/Perspective/org-netbeans-modules-perspective-actions-QuickSwitch.instance"/>
        </file>
    </folder>
    <folder name="perspectives">
    </folder>
    <!--window system config folder-->
    <folder name="Windows2">
        <!--Tc components folder-->
        <folder name="Components">
            <!--Modehack Tc Settings file -->
            <file name="ModeHackTopComponent.settings" url="hacks/ModeHackTopComponentSettings.xml"/>
        </folder>
    </folder>
</filesystem>
