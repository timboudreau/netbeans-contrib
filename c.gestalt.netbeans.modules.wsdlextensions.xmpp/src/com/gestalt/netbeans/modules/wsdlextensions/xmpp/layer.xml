<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">
<filesystem>
    <folder name="WSDLEditor">
        
        <file name="XMPPWsdlExt.xsd" url="XMPPWsdlExt.xsd">
            <attr name="namespace" stringvalue="http://schemas.sun.com/jbi/wsdl-extensions/xmpp/" />
            <attr name="prefix" stringvalue="xmpp" />
        </file>
        
        <folder name ="Binding">
            <file name="XMPPBinding">
                <attr name="xsdFileName" stringvalue="XMPPWsdlExt" />
                <attr name="element" stringvalue="binding" />
                <attr name="SystemFileSystem.localizingBundle" stringvalue="com.gestalt.netbeans.modules.wsdlextensions.xmpp.Bundle" />
                <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/com/gestalt/netbeans/modules/wsdlextensions/xmpp/resources/binding.png" /> 
            </file>
        </folder>
        
        <folder name ="BindingOperation">
            <file name="XMPPOperation">
                <attr name="xsdFileName" stringvalue="XMPPWsdlExt" />
                <attr name="element" stringvalue="operation" />
                <attr name="SystemFileSystem.localizingBundle" stringvalue="com.gestalt.netbeans.modules.wsdlextensions.xmpp.Bundle" />
                <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/com/gestalt/netbeans/modules/wsdlextensions/xmpp/resources/operation.png" /> 
            </file>
        </folder>
        
        <folder name ="BindingOperationInput">
            <file name="XMPPInMessage">
                <attr name="xsdFileName" stringvalue="XMPPWsdlExt" />
                <attr name="element" stringvalue="input" />
                <attr name="SystemFileSystem.localizingBundle" stringvalue="com.gestalt.netbeans.modules.wsdlextensions.xmpp.Bundle" />
                <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/com/gestalt/netbeans/modules/wsdlextensions/xmpp/resources/inMessage.png" /> 
            </file>
        </folder>	        
        
        <folder name ="BindingOperationOutput">
            <file name="XMPPOutMessage">
                <attr name="xsdFileName" stringvalue="XMPPWsdlExt" />
                <attr name="element" stringvalue="output" />
                <attr name="SystemFileSystem.localizingBundle" stringvalue="com.gestalt.netbeans.modules.wsdlextensions.xmpp.Bundle" />
                <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/com/gestalt/netbeans/modules/wsdlextensions/xmpp/resources/outMessage.png" /> 
            </file>
        </folder>	        
        
        <folder name ="BindingOperationFault">
            <file name="XMPPFault">
                <attr name="xsdFileName" stringvalue="XMPPWsdlExt" />
                <attr name="element" stringvalue="fault" />
                <attr name="SystemFileSystem.localizingBundle" stringvalue="com.gestalt.netbeans.modules.wsdlextensions.xmpp.Bundle" />
                <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/com/gestalt/netbeans/modules/wsdlextensions/xmpp/resources/fault.png" /> 
            </file>            
        </folder>
        
        <folder name ="ServicePort">
            <file name="XMPPPort" >
                <attr name="xsdFileName" stringvalue="XMPPWsdlExt" />
                <attr name="element" stringvalue="address" />
                <attr name="SystemFileSystem.localizingBundle" stringvalue="com.gestalt.netbeans.modules.wsdlextensions.xmpp.Bundle" />
                <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/com/gestalt/netbeans/modules/wsdlextensions/xmpp/resources/port.png" /> 
            </file>
        </folder>	        
    </folder>
    
    <folder name="SeeBeyondJbiComponents">
        <!-- JBI Component -->
        <folder name="gestalt-xmpp-binding">
            <attr name="id" stringvalue="gestalt-xmpp-binding"/>
            <attr name="type" stringvalue="Binding"/>
            <attr name="description" stringvalue="Description of binding component : XMPP"/>
            <file name="xmpp.binding-1.0" >
                <attr name="namespace" stringvalue="http://schemas.sun.com/jbi/wsdl-extensions/xmpp/"/>
            </file>
        </folder>
    </folder>
    
</filesystem>
