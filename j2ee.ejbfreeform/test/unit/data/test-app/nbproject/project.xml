<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.ant.freeform</type>
    <configuration>
        <ejb-data xmlns="http://www.netbeans.org/ns/freeform-project-ejb/1">
            <ejb-module>
                <config-files>conf</config-files>
                <classpath/>
                <j2ee-spec-level>1.4</j2ee-spec-level>
            </ejb-module>
        </ejb-data>
        <general-data xmlns="http://www.netbeans.org/ns/freeform-project/1">
            <!-- Do not use Project Properties customizer when editing this file manually. -->
            <name>test-app</name>
            <properties>
                <property name="resource.dir">resources</property>
                <property name="j2ee.platform">1.4</property>
                <property name="j2ee.server.type">GENERIC</property>
            </properties>
            <folders>
                <source-folder>
                    <label>conf</label>
                    <type>configFilesRoot</type>
                    <location>conf</location>
                </source-folder>
                <source-folder>
                    <label>src\beans</label>
                    <type>java</type>
                    <location>src/beans</location>
                </source-folder>
                <source-folder>
                    <label>src\java</label>
                    <type>java</type>
                    <location>src/java</location>
                </source-folder>
                <source-folder>
                    <label>ws</label>
                    <type>java</type>
                    <location>src/ws</location>
                </source-folder>
                <source-folder>
                    <label>test</label>
                    <type>java</type>
                    <location>test</location>
                </source-folder>
            </folders>
            <ide-actions>
                <action name="build">
                    <target>build</target>
                </action>
                <action name="clean">
                    <target>clean</target>
                </action>
                <action name="javadoc">
                    <target>javadoc</target>
                </action>
                <action name="run">
                    <target>run</target>
                </action>
                <action name="test">
                    <target>test</target>
                </action>
                <action name="deploy">
                    <target>deploy</target>
                </action>
                <action name="rebuild">
                    <target>clean</target>
                    <target>build</target>
                </action>
            </ide-actions>
            <view>
                <items>
                    <source-folder style="ejbs">
                        <label>Enterprise Beans</label>
                        <location>src/beans</location>
                    </source-folder>
                    <source-folder style="configFiles">
                        <label>Configuration Files</label>
                        <location>conf</location>
                    </source-folder>
                    <source-folder style="packages">
                        <label>src\beans</label>
                        <location>src/beans</location>
                    </source-folder>
                    <source-folder style="packages">
                        <label>src\java</label>
                        <location>src/java</location>
                    </source-folder>
                    <source-folder style="packages">
                        <label>ws</label>
                        <location>src/ws</location>
                    </source-folder>
                    <source-folder style="packages">
                        <label>test</label>
                        <location>test</location>
                    </source-folder>
                    <source-file>
                        <location>build.xml</location>
                    </source-file>
                </items>
                <context-menu>
                    <ide-action name="build"/>
                    <ide-action name="clean"/>
                    <ide-action name="javadoc"/>
                    <ide-action name="run"/>
                    <ide-action name="test"/>
                    <ide-action name="deploy"/>
                    <ide-action name="rebuild"/>
                </context-menu>
            </view>
            <subprojects/>
        </general-data>
        <java-data xmlns="http://www.netbeans.org/ns/freeform-project-java/2">
            <compilation-unit>
                <package-root>src/ws</package-root>
                <source-level>1.4</source-level>
            </compilation-unit>
            <compilation-unit>
                <package-root>src/beans</package-root>
                <source-level>1.4</source-level>
            </compilation-unit>
            <compilation-unit>
                <package-root>src/java</package-root>
                <classpath mode="compile">lib/test-lib1.jar</classpath>
                <source-level>1.4</source-level>
            </compilation-unit>
            <compilation-unit>
                <package-root>test</package-root>
                <unit-tests/>
                <classpath mode="compile">lib/test-lib1.jar</classpath>
                <source-level>1.4</source-level>
            </compilation-unit>
        </java-data>
    </configuration>
</project>
